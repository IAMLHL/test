@{
    ViewBag.Title = "Home Page";
}
<script src="@Url.Content("~/Scripts/jquery-1.10.2.js")"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<form action="/Home/Addtest" method="post">
    <nav class="navbar navbar-default" role="navigation">
        <div class="jumbotron">
            <h1>ASP.NET</h1>
            <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
            <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
        </div>

        <div class="row">
            <div class="col-md-4">
                @model TestProgram.Models.Product
                <p>
                    名称：<input type="text" name="Name" value="@Model.Name" />
                </p>
                <p>
                    价格：
                    @Html.TextBoxFor(u => u.Price)
                    @Html.ValidationMessageFor(u => u.Price)
                </p>
                <p>数量：<input type="text" name="Number" value="@Model.Number" /></p>
                @ViewData["Price"]
                @ViewBag.Name
                @Html.DropDownList("ddl_sss")

                <ul class="nav nav-tabs">
                    <li class="active"><a href="#">iOS</a></li>
                    <li><a href="#">SVN</a></li>
                    <li><a href="#">1111</a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                            Java
                            <b class="caret"></b>
                        </a>
                        <ul class="dropdown-menu">
                            <li><a href="#">jmeter</a></li>
                            <li><a href="#">EJB</a></li>
                            <li><a href="#">Jasper Report</a></li>
                            <li class="divider"></li>
                            <li><a href="#">分离的链接</a></li>
                            <li class="divider"></li>
                            <li><a href="#">另一个分离的链接</a></li>
                        </ul>
                    </li>
                </ul>
                <p><input type="submit" value="提交" /></p>
            </div>
        </div>
        </nav>
</form>
